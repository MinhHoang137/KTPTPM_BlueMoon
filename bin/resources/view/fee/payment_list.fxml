<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns:fx="http://javafx.com/fxml"
            fx:controller="controller.fee.PaymentListController"
            prefWidth="800" prefHeight="500">

    <TableView fx:id="tablePayments" layoutX="20" layoutY="20" prefWidth="760" prefHeight="380" />

    <Button layoutX="20" layoutY="420" text="Thêm mới" onAction="#onAddClicked"/>
    <Button layoutX="120" layoutY="420" text="Sửa" onAction="#onEditClicked"/>
    <Button layoutX="220" layoutY="420" text="Xóa" onAction="#onDeleteClicked"/>
    <Button layoutX="320.0" layoutY="420.0" text="Quay lại" onAction="#onBackClicked"/>


</AnchorPane>
