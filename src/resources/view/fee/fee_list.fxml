<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns:fx="http://javafx.com/fxml" fx:controller="controller.fee.FeeListController"
            prefWidth="600.0" prefHeight="400.0">
    
    <!-- TableView hiển thị danh sách khoản thu -->
    <TableView fx:id="tableFeeList" layoutX="20.0" layoutY="20.0" prefWidth="560.0" prefHeight="300.0" />
    
    <!-- Nút Thêm -->
    <Button layoutX="20.0" layoutY="340.0" mnemonicParsing="false"
            onAction="#onAddClicked" text="Thêm mới" />
    
    <!-- Nút Sửa -->
    <Button layoutX="120.0" layoutY="340.0" mnemonicParsing="false"
            onAction="#onEditClicked" text="Sửa" />
    
    <!-- Nút Xóa -->
    <Button layoutX="200.0" layoutY="340.0" mnemonicParsing="false" onAction="#onDeleteClicked" text="Xóa" />
    <!--Back-->
    <Button layoutX="320.0" layoutY="420.0" text="Quay lại" onAction="#onBackClicked"/>





</AnchorPane>
